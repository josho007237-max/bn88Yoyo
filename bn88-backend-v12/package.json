{
  "name": "bn9-backend-v2",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev:prep": "prisma generate && prisma db push && tsx src/scripts/seed.ts",
    "dev:backend": "tsx watch src/server.ts",
    "dev:tunnel": "cloudflared tunnel run bn9-api",
    "dev:frontend": "cd ../bn9-frontend-dashboard-v2 && npm run dev",
    "dev:all": "run-p dev:backend dev:tunnel dev:frontend",
    "dev:full": "npm run dev:prep && npm run dev:all",
    "check:all": "pwsh -ExecutionPolicy Bypass -File ./src/scripts/dev-check.ps1",
    "check:line": "pwsh -ExecutionPolicy Bypass -File ./src/scripts/dev-check-line.ps1",
    "dev:check:chat": "tsx src/scripts/dev-check-chat.ts",
    "seed": "tsx src/scripts/seed.ts",
    "seed:admin": "tsx src/scripts/seedAdmin.ts",
    "seed:bot": "tsx src/scripts/seedBot.ts",
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "postinstall": "prisma generate",
    "start": "node dist/server.js",
    "migrate:deploy": "prisma migrate deploy",
    "check:extended": "pwsh -ExecutionPolicy Bypass -File ./src/scripts/dev-check-extended.ps1"
  },
  "dependencies": {
    "@line/bot-sdk": "^10.5.0",
    "@prisma/client": "^6.19.0",
    "@types/axios": "^0.14.0",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "openai": "^6.8.1",
    "recharts": "^3.3.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.24",
    "concurrently": "^9.2.1",
    "npm-run-all": "^4.1.5",
    "prisma": "^6.19.0",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
