    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.0.0",
    "node-cron": "^3.0.2",
    "pg": "^8.10.0",
    "querystring": "^0.2.1",
    "redis": "^4.6.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1",
